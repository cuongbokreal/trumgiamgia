get=&quot;_blank&quot;&gt;Xem trang SuperIdol của bạn&lt;/a&gt;&lt;/p&gt;&lt;/div&gt; &lt;a class=&quot;tags&quot;&gt;Super Idol&lt;/a&gt;&lt;script&gt;  function getlink() {          var name = document.getElementById(&quot;name&quot;).value;     var content = document.getElementById(&quot;content&quot;).value;           myInput.value = &#39;https://cuongbokit.blogspot.com/p/co-dieu-bat-ngo.html?name=&#39; + name + &#39;&amp;content=&#39; + content;               var linkbio = document.getElementById(&#39;myInput&#39;).value;     if(name.length == 0){document.getElementById(&#39;thbao&#39;).innerHTML = &#39;⚠️ Bạn hãy nhập tên! ⚠️&#39;;}     if(content.length == 0){document.getElementById(&#39;thbao&#39;).innerHTML = &#39;⚠️ Bạn hãy nhập Chú thích! ⚠️&#39;;}           var result = document.getElementById(&#39;myInput&#39;).value;          if(result.length &gt;= 2 &amp;&amp; name){document.getElementById(&#39;thbao&#39;).innerHTML = &#39;😍 Thành công, đã sao chép liên kết, chúc bạn trải nghiệm vui vẻ 😍&#39;; console.log(linkbio);getin4.href = linkbio;window.open(linkbio);                                   document.getElementById(&#39;getin4&#39;).innerHTML =&#39;Xem trang Super Idol của &#39; + name.replaceAll(&quot; &quot;,&quot;%20&quot;) + &#39;, nội dung: &#39; + content.replaceAll(&quot; &quot;,&quot;%20&quot;)}        /* Get the text field */   var copyText = document.getElementById(&quot;myInput&quot;);   /* Select the text field */   copyText.select();   copyText.setSelectionRange(0, 999); /* For mobile devices */   /* Copy the text inside the text field */   document.execCommand(&quot;copy&quot;);       /* Alert the copied text */   console.log(&quot;Copied the text: &quot; + copyText.value); } &lt;/script&gt; &lt;style&gt;  #4CAF50   div{padding:1px} input {   font-family: &quot;Roboto&quot;, sans-serif;   outline: 0;   background: #f2f2f2;   width: 100%;   border: 0;   margin: 0 0 15px;   padding: 15px;   box-sizing: border-box;   font-size: 14px; }    .b